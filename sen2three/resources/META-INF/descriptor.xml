<?xml version="1.0" encoding="windows-1252"?>
<operator>
  <name>org.esa.s2tbx.sen2three</name>
  <operatorClass>org.esa.snap.core.gpf.operators.tooladapter.ToolAdapterOp</operatorClass>
  <alias>Sen2Three</alias>
  <label>SEN2THREE</label>
  <version>0.9.0</version>
  <description>Sen2Three Processor</description>
  <authors/>
  <copyright/>
  <internal>false</internal>
  <autoWriteSuppressed>false</autoWriteSuppressed>
  <menuLocation>Menu/Optical/Thematic Land Processing</menuLocation>
  <preprocessTool>false</preprocessTool>
  <writeForProcessing>false</writeForProcessing>
  <mainToolFileLocation>$PYTHON_BIN</mainToolFileLocation>
  <workingDir>$SEN2THREE_HOME</workingDir>
  <templateType>VELOCITY</templateType>
  <template type="file">
    <file>Sen2Three-template.vm</file>
  </template>
  <progressPattern>Progress\[%\]: (\d{1,3})\.(?:.+)</progressPattern>
  <errorPattern>[\s\S]+(?:UserWarning|Error): (.+)</errorPattern>
  <variables>
    <variable>
      <key>SEN2THREE_HOME</key>
      <value/>
      <isShared>false</isShared>
    </variable>
    <variable>
      <key>SEN2THREE_BIN</key>
      <value/>
      <isShared>false</isShared>
    </variable>
    <osvariable>
      <key>PYTHON_BIN</key>
      <value>python</value>
      <isShared>false</isShared>
      <windows>python.exe</windows>
      <linux>python</linux>
      <macosx>python</macosx>
      <isTransient>false</isTransient>
    </osvariable>
  </variables>
  <parameters>
    <parameter>
      <name>targetProductFile</name>
      <dataType>java.io.File</dataType>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <templateparameter>
      <name>postExecuteTemplate</name>
      <dataType>java.io.File</dataType>
      <defaultValue>Sen2Three-post-template.vm</defaultValue>
      <description/>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>TemplateAfterExecution</parameterType>
      <template type="file">
        <file>Sen2Three-post-template.vm</file>
      </template>
      <parameters/>
    </templateparameter>
    <parameter>
      <name>clean</name>
      <dataType>java.lang.Boolean</dataType>
      <description>Clean</description>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>resolution</name>
      <dataType>java.lang.Integer</dataType>
      <defaultValue>60</defaultValue>
      <valueSet>
        <string>10</string>
        <string>20</string>
        <string>60</string>
      </valueSet>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>sourceFolder</name>
      <dataType>java.io.File</dataType>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>FolderParameter</parameterType>
    </parameter>
  </parameters>
  <source>user</source>
  <isSystem>false</isSystem>
  <isHandlingOutputName>true</isHandlingOutputName>
  <sourceProductDescriptors/>
  <targetPropertyDescriptors/>
  <numSourceProducts>0</numSourceProducts>
</operator>