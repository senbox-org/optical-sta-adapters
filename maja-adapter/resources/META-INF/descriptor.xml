<operator>
  <name>org.esa.s2tbx.maja</name>
  <operatorClass>org.esa.snap.core.gpf.operators.tooladapter.ToolAdapterOp</operatorClass>
  <alias>Maja</alias>
  <label>maja</label>
  <version>1.0.0</version>
  <description>Tool adapter for Maja using startmaja command line</description>
  <authors>Florian Douziech</authors>
  <copyright></copyright>
  <internal>false</internal>
  <autoWriteSuppressed>false</autoWriteSuppressed>
  <menuLocation>Menu/Optical/Thematic Land Processing</menuLocation>
  <preprocessTool>false</preprocessTool>
  <writeForProcessing>false</writeForProcessing>
  <mainToolFileLocation>$MAJA_BIN_DIR/bin/$MAJA_EXEC</mainToolFileLocation>
  <workingDir>$java.io.tmpdir</workingDir>
  <template type="file">
    <file>Maja-template.vm</file>
  </template>
  <progressPattern></progressPattern>
  <errorPattern>(^.*Error|^.*Exception)(.+)</errorPattern>
  <stepPattern>workplan #(.+)</stepPattern>
  <variables>
    <osvariable>
      <key>MAJA_BIN_DIR</key>
      <value/>
      <windows></windows>
      <linux>$HOME/.snap/auxdata/MAJA-4.5.3</linux>
      <macosx></macosx>
      <isTransient>true</isTransient>
      <isShared>false</isShared>
    </osvariable>
    <osvariable>
      <key>MAJA_EXEC</key>
      <value/>
      <windows></windows>
      <linux>startmaja</linux>
      <macosx></macosx>
      <isTransient>false</isTransient>
      <isShared>false</isShared>
    </osvariable>
    <osvariable>
      <key>USERPROFILE</key>
      <value></value>
      <isShared>false</isShared>
      <windows></windows>
      <linux>$HOME</linux>
      <macosx></macosx>
      <isTransient>false</isTransient>
    </osvariable>
    <osvariable>
      <key>HOME</key>
      <value></value>
      <isShared>false</isShared>
      <windows>$USERPROFILE</windows>
      <linux></linux>
      <macosx></macosx>
      <isTransient>false</isTransient>
    </osvariable>
    <osvariable>
      <key>USER_HOME</key>
      <value></value>
      <isShared>false</isShared>
      <windows></windows>
      <linux>$HOME</linux>
      <macosx></macosx>
      <isTransient>false</isTransient>
    </osvariable>
    <variable>
      <key>java.io.tmpdir</key>
      <value></value>
      <isShared>false</isShared>
    </variable>
  </variables>
  <parameters>
    <parameter>
      <name>TILE</name>
      <dataType>java.lang.String</dataType>
      <defaultValue></defaultValue>
      <description>Tile number</description>
      <label>TILENUMBER</label>
      <valueSet/>
      <notNull>true</notNull>
      <notEmpty>true</notEmpty>
      <deprecated>false</deprecated>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>FOLDER</name>
      <dataType>java.io.File</dataType>
      <defaultValue></defaultValue>
      <description>Config/Folder-definition file used for all permanent paths.</description>
      <label>FOLDER</label>
      <notNull>true</notNull>
      <notEmpty>true</notEmpty>
      <deprecated>false</deprecated>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>SITE</name>
      <dataType>java.lang.String</dataType>
      <description>Site name. If not specified,the tile number is used directly for finding the L1/L2 product directory</description>
      <label>SITENAME</label>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <deprecated>false</deprecated>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>START</name>
      <dataType>java.lang.String</dataType>
      <description>Start date for processing in format YYYY-MM-DD. If none is provided,all products until the end date will be processed</description>
      <label>STARTDATE</label>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <deprecated>false</deprecated>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>END</name>
      <dataType>java.lang.String</dataType>
      <description>Error: Exception on getting the value of the attribute &apos;description&apos; message: null</description>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <deprecated>false</deprecated>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>NBACKWARD</name>
      <dataType>java.lang.Integer</dataType>
      <description>Number of products used to run in backward mode. Default is 8.</description>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>verbose</name>
      <dataType>java.lang.Boolean</dataType>
      <description>Provides detailed (DEBUG) logging for Maja. Default is false</description>
      <label></label>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <deprecated>false</deprecated>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>overwrite</name>
      <dataType>java.lang.Boolean</dataType>
      <defaultValue>false</defaultValue>
      <description>Overwrite existing L2 products. Default is false.</description>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>skipErrors</name>
      <dataType>java.lang.Boolean</dataType>
      <description>Skip erroneous products without stopping.</description>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>CAMS</name>
      <dataType>java.lang.String</dataType>
      <description>Number of products used to run in backward mode.Use CAMS during processing.The CAMS files have to be available in the repCAMS dir. Default is False.Default is 8.</description>
      <label>CAMS</label>
      <valueSet/>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>TYPEDEM</name>
      <dataType>[Ljava.lang.String;</dataType>
      <defaultValue>any</defaultValue>
      <description>DEM type. If none is given, any will be used</description>
      <interval></interval>
      <valueSet>
        <string>srtm</string>
        <string>eudem</string>
        <string>any</string>
      </valueSet>
      <condition></condition>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <deprecated>false</deprecated>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>PLATFORM</name>
      <dataType>[Ljava.lang.String;</dataType>
      <defaultValue>auto</defaultValue>
      <description> Manually override which platform to use.By default this is deducted by the available input product(s)</description>
      <valueSet>
        <string>sentinel2</string>
        <string>landsat8</string>
        <string>venus</string>
        <string>auto</string>
      </valueSet>
      <condition></condition>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <deprecated>false</deprecated>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>OUTPUT_FORMAT</name>
      <dataType>[Ljava.lang.String;</dataType>
      <defaultValue>natif</defaultValue>
      <description>Output product type to use to write L2 product in native or muscate format, the default is muscate format</description>
      <valueSet>
        <string>natif</string>
        <string>muscate</string>
      </valueSet>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <deprecated>false</deprecated>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>USERCONF</name>
      <dataType>java.lang.String</dataType>
      <defaultValue></defaultValue>
      <description>User Configuration directory, contains for example MAJAUserConfigSystem.xml</description>
      <label>USERCONF</label>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <deprecated>false</deprecated>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>MAX_L2_DIFF</name>
      <dataType>java.lang.Integer</dataType>
      <description>Max timedelta in days between an L1C and L2 product to be considered for a NOMINAL Workplan. By default this is handled independently depending on each platform (i.e. S2=45days).</description>
      <label>MAX_L2_DIFF</label>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <deprecated>false</deprecated>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <parameter>
      <name>targetProductFile</name>
      <dataType>java.io.File</dataType>
      <description>Output file</description>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>RegularParameter</parameterType>
    </parameter>
    <templateparameter>
      <name>postExecuteTemplate</name>
      <dataType>java.io.File</dataType>
      <defaultValue>Maja-post-template.vm</defaultValue>
      <description></description>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <parameterType>TemplateAfterExecution</parameterType>
      <parameters>
        <parameter>
          <name>targetFolder</name>
          <defaultValue></defaultValue>
          <description>The folder of the L2A data product</description>
          <dataType>java.io.File</dataType>
          <valueSet/>
          <notNull>false</notNull>
          <notEmpty>false</notEmpty>
          <parameterType>RegularParameter</parameterType>
        </parameter>
      </parameters>
      <template type="file">
        <file>Maja-post-template.vm</file>
      </template>
    </templateparameter>
  </parameters>
  <source>user</source>
  <isSystem>false</isSystem>
  <isHandlingOutputName>true</isHandlingOutputName>
  <windowsBundle>
    <bundleType>NONE</bundleType>
    <bundleLocation>REMOTE</bundleLocation>
  </windowsBundle>
  <linuxBundle>
    <bundleType>INSTALLER</bundleType>
    <bundleLocation>REMOTE</bundleLocation>
    <downloadURL>http://step.esa.int/thirdparties/maja/4.5.3/MAJA-4.5.3.run</downloadURL>
    <templateparameter>
      <name>arguments</name>
      <dataType>java.io.File</dataType>
      <defaultValue>Command Template</defaultValue>
      <valueSet/>
      <notNull>false</notNull>
      <notEmpty>false</notEmpty>
      <deprecated>false</deprecated>
      <parameterType>TemplateParameter</parameterType>
      <parameters>
        <parameter>
          <name>targetLocation</name>
          <dataType>java.io.File</dataType>
          <defaultValue>$USERPROFILE/.snap/auxdata</defaultValue>
          <valueSet/>
          <notNull>false</notNull>
          <notEmpty>false</notEmpty>
          <parameterType>RegularParameter</parameterType>
        </parameter>
      </parameters>
      <template type="simple">
        <contents>--nox11
          --target
          $targetLocation/MAJA-4.5.3</contents>
      </template>
      <outputFile>Command Template</outputFile>
    </templateparameter>
    <targetLocation>$USERPROFILE/.snap/auxdata</targetLocation>
    <entryPoint>MAJA-4.5.3.run</entryPoint>
    <updateVariable>MAJA_BIN_DIR</updateVariable>
  </linuxBundle>
  <macosxBundle>
    <bundleType>NONE</bundleType>
    <bundleLocation>REMOTE</bundleLocation>
  </macosxBundle>
  <sourceProductDescriptors/>
  <targetPropertyDescriptors/>
  <templateType>VELOCITY</templateType>
  <numSourceProducts>0</numSourceProducts>
</operator>